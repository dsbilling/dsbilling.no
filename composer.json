{
    "name": "danielrtrd/daniel.rtrd.no",
    "type": "project",
    "description": "Just my personal website",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^2.1",
        "cyrildewit/eloquent-viewable": "^7.0",
        "doctrine/dbal": "^3.5",
        "filament/filament": "^3.3",
        "filament/spatie-laravel-tags-plugin": "^3.3",
        "graham-campbell/markdown": "^15.2",
        "kilobyteno/laravel-user-guest-like": "^1.4.0",
        "laravel/framework": "^11.44",
        "laravel/jetstream": "^5.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.3",
        "mtownsend/read-time": "^2.0",
        "romanzipp/laravel-seo": "^2.7",
        "sentry/sentry-laravel": "^4.3",
        "spatie/laravel-honeypot": "^4.5",
        "spatie/laravel-permission": "^6.2",
        "spatie/laravel-schedule-monitor": "^3.7",
        "spatie/laravel-tags": "^4.6",
        "torchlight/torchlight-commonmark": "dev-main"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
         {
             "type": "vcs",
             "url": "https://github.com/caendesilva/torchlight-commonmark-php"
         }
     ]
}
